{
  "expo": {
    "name": "client",
    "slug": "client",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#9c0f97"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*.png", "**/*.svg"],
    "ios": {
      "supportsTablet": true
    }
  }
}
